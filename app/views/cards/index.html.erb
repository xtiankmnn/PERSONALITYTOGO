<h1>Cards#index</h1>
<p>Find me in app/views/cards/index.html.erb</p>

<div class="container">
  <div class="row">
    <% 16.times do |i| %>
      <div class="col-md-3 mb-4">
        <div class="card" style="width: 18rem;">
          <img src="https://via.placeholder.com/150" class="card-img-top" alt="Card image <%= i + 1 %>">
          <div class="card-body">
            <h5 class="card-title">MTBI <%= i + 1 %></h5>
            <p class="card-text">Description</p>
            <a href="#" class="btn btn-primary">Learn more</a>
          </div>
        </div>
      </div>
      <% if (i + 1) % 4 == 0 %>
        </div><div class="row">
      <% end %>
    <% end %>
  </div>
</div>
